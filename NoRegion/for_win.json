{
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip",
      "external_ui_download_detour": "Default",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true
    }
  },
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "proxy_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "Default"
      },
      {
        "tag": "cn_dns",
        "type": "udp",
        "server": "114.114.114.114"
      },
      {
        "tag": "ai_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "AI-Service"
      },
      {
        "tag": "media_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "Streaming-Media"
      },
      {
        "tag": "tiktok_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "TikTok"
      },
      {
        "tag": "paypal_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "PayPal"
      },
      {
        "tag": "telegram_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "Telegram"
      },
      {
        "tag": "emby_dns",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "BlackCat-EMBY"
      }
    ],
    "rules": [
      {
        "domain_suffix": [ "uuuoo.cc", "emby.bid", "emby.date", "emby.loan", "emby.trade" ],
        "server": "emby_dns"
      },
      {
        "rule_set": "telegramsite",
        "server": "telegram_dns"
      },
      {
        "rule_set": "tiktok",
        "server": "tiktok_dns"
      },
      {
        "rule_set": "paypal",
        "server": "paypal_dns"
      },
      {
        "rule_set": "aiservice",
        "server": "ai_dns"
      },
      {
        "rule_set": [ "netflixsite", "hbo", "disney", "hulu" ],
        "server": "media_dns"
      },
      {
        "rule_set": "gfw",
        "server": "proxy_dns"
      },
      {
        "rule_set": "bilibili",
        "server": "cn_dns"
      }
    ],
    "final": "cn_dns",
    "disable_cache": false,
    "independent_cache": true,
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "singbox_tun",
      "address": [ "172.18.0.1/30" ],
      "auto_route": true,
      "strict_route": true
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "::",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Default",
      "outbounds": [ "Auto" ]
    },
    {
      "type": "urltest",
      "tag": "Auto",
      "outbounds": [ ],
      "interval": "3m",
      "tolerance": 30
    },
    {
      "type": "selector",
      "tag": "Relay",
      "outbounds": [ "Direct-Out" ]
    },

    {
      "type": "selector",
      "tag": "PayPal",
      "outbounds": [ "Default", "Auto" ]
    },
    {
      "type": "selector",
      "tag": "AI-Service",
      "outbounds": [ "Default", "Auto" ]
    },
    {
      "type": "selector",
      "tag": "Streaming-Media",
      "outbounds": [ "Default", "Auto" ]
    },
    {
      "type": "selector",
      "tag": "TikTok",
      "outbounds": [ "Default", "Auto" ]
    },

    {
      "type": "selector",
      "tag": "Telegram",
      "outbounds": [ "Default", "Auto" ]
    },
    {
      "type": "selector",
      "tag": "BlackCat-EMBY",
      "outbounds": [ "Default", "Auto" ]
    },
    {
      "type": "selector",
      "tag": "BiliBili",
      "outbounds": [ "Direct-Out", "Default", "Auto" ]
    },
    {
      "type": "direct",
      "tag": "Direct-Out"
    }
  ],
  "route": {
    "final": "Default",
    "auto_detect_interface": true,
    "default_domain_resolver": "cn_dns",
    "rule_set": [
      {
        "tag": "privatesite",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/private.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "privateip",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/private.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "cnip",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/cn.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "cngames",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/category-games-!cn@cn.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "gfw",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/gfw.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "aiservice",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/category-ai-!cn.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "paypal",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/paypal.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "tiktok",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/tiktok.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "telegramip",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/telegram.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "telegramsite",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/telegram.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "netflixsite",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/netflix.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "netflixip",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geoip/netflix.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "disney",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/disney.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "hbo",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/hbo.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "hulu",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/hulu.srs",
        "update_interval": "24h",
        "download_detour": "Default"
      },
      {
        "tag": "bilibili",
