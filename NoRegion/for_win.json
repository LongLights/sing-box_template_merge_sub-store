        "update_interval": "24h",
        "download_detour": "Default"
      }
    ],
    "rules": [
      {
        "action": "sniff"
      },
      {
        "action": "resolve"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "Direct-Out"
      },
      {
        "clash_mode": "global",
        "outbound": "Default"
      },
      {
        "domain_suffix": [ "uuuoo.cc", "emby.bid", "emby.date", "emby.loan", "emby.trade" ],
        "outbound": "BlackCat-EMBY"
      },
      {
        "rule_set": "paypal",
        "outbound": "PayPal"
      },
      {
        "rule_set": "telegramsite",
        "outbound": "Telegram"
      },
      {
        "rule_set": "tiktok",
        "outbound": "TikTok"
      },
      {
        "rule_set": "aiservice",
        "outbound": "AI-Service"
      },
      {
        "rule_set": "bilibili",
        "outbound": "BiliBili"
      },
      {
        "rule_set": [ "netflixsite", "hbo", "disney", "hulu" ],
        "outbound": "Streaming-Media"
      },
      {
        "rule_set": "gfw",
        "outbound": "Default"
      },
      {
        "rule_set": [ "privatesite", "cngames" ],
        "outbound": "Direct-Out"
      },
      {
        "rule_set": "telegramip",
        "outbound": "Telegram"
      },
      {
        "rule_set": "netflixip",
        "outbound": "Streaming-Media"
      },
      {
        "rule_set": [ "privateip", "cnip" ],
