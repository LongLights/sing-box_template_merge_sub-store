        },
        {
          "tag": "cn_dns",
          "type": "udp",
          "server": "114.114.114.114"
        }
      ],
      "rules": [
        {
          "rule_set": "gfw",
          "server": "proxy_dns"
        },
        {
          "type": "logical",
          "mode": "and",
          "rules": [
            {
              "rule_set": "geosite-geolocation-!cn",
